<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rathana.adapter_demo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".CountryActivity" />
        <activity android:name=".SongListActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".AmsActivity"></activity>
        <activity android:name=".AddAmsActivity" />
        <activity android:name=".adapter.GridLayoutManagerActivity">

        </activity>
        <activity android:name=".StaggeredGridActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>